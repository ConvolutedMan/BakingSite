



class allRecipes{
    constructor(name, site){
        this._name = name;
        this._site= "../../"+site;
        
        
    }
    get name() {
        return this._name;
    }
    get site() {
        return this._site;
    }
    
   
} 

class Dessert extends allRecipes{
    constructor(name, site){
    super(name, site);
    this.type = "Dessert"
    }
}

class Appetizer extends allRecipes{
    constructor(name, site){
    super(name, site);
    this.type = "Appetizer"
    }
}

class Entree extends allRecipes{
